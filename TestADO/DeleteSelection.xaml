<Window x:Class="TestADO.DeleteSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestADO"
        mc:Ignorable="d"
        Title="DeleteSelection" Height="300" Width="300">
    <Grid>
        <Label Height="30" Width="150" Margin="0 -210 0 0" Content="Id of File" HorizontalContentAlignment="Center"></Label>
        <ComboBox Height="30" Width="100" Margin="0 -100 0 0" Name="userFilesBox"></ComboBox>
        <Button Width="80" Height="30" Content="Delete" Click="DeleteUserFile"></Button>
        <Button Width="80" Height="30" Content="Back" Click="BackClick" Margin="10,25,202,214"></Button>

    </Grid>
</Window>
